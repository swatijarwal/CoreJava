package exception;

public class Arithmetic {

	public static void main(String[] args) {
		try {
			
			String name="swati";
			int a=10;
			int b=0;
			 int c= a/b;
			 System.out.println(name);
			 System.out.println(c);
			
		} catch (Exception e) {
			System.out.println("catch block");
		}

	}

}
