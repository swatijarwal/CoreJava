package cloning;

public class Person {
	private String name=null;
	public Account account=null;
	public Person(String name)
	{
	  this.name=name;
	  this.account= new Account(100);
	  
	}
	public void setName(String name) {
		this.name = name;
	}
	  
	public String getName() {
		return name;
	}
	

}
