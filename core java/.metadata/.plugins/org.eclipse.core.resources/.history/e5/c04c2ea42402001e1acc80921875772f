package basic;

public class ExtractLastName {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		String[] s={"vijay","chauhan"};
		
		
		for(int i=0; i<s.length;i++)
		{
		 	 for (int j = 0; s.length<j; j--) {
				
			 
			System.out.print(s[i] .charAt(j));
		}
				

	}

}
}