package cloning;

public class Employee {
 private int id;
 private String name;
 private String add;
 public Employee(String name)
 {
	this.id=id;
	this.name=name;
	this.add=add;
 }
 public void setId(int id)
 {
	 this.id=id;
 }
 public int getId() {
	return id;
}
 public void setName(String name) {
	this.name = name;
}
 public String getName() {
	return name;
}
 public void setAdd(String add) {
	this.add = add;
}
 public String getAdd() {
	return add;
}
}
