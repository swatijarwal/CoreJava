package cloning;

public class TextCloning {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		Account a1= new Account(100);
	     Account   a2=a1;
	       a2.setBalance(20);
	     
	     System.out.println(a1.getBalance());
	     System.out.println(a2.getBalance());
		
          
	}

}
