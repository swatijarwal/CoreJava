package basic;

public class Su {

	public static void main(String[] args) {
		// TODO Auto-generated method stub

		int j=0;
		for( int i=100;i<=200; i++)
		{
			  while(i/7==0)
			  {
				  j=i+j;
				  System. out.print(i);
				  
			  }
		} System.out.println("sum of all integer"+j);
		
		
		
	}

}
